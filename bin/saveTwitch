#!/usr/bin/env node

var client = require('../models/db').client();
var twitch = require('../api/twitch');

var Snapshot = require('../models/Snapshot');

var handleErr = (err) => {
    console.log(err);
};

new Snapshot().getFromTwitch()
    .then((snapshot) => {
        return snapshot.save();
    }, handleErr)
    .then(() => {}, handleErr);
