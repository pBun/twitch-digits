#!/usr/bin/env node

var client = require('../models/client');
var twitch = require('../api/twitch');

var Snapshot = require('../models/Snapshot');

var handleErr = (err) => {
    console.log(err);
};

client.connect();
var snapshot = new Snapshot();
snapshot.getFromTwitch().then(() => {
    return snapshot.save();
}, handleErr)
.then(() => {
    client.end();
}, (err) => {
    handleErr(err);
    client.end();
});
